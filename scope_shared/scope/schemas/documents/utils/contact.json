	"$id": "https://uwscope.org/schemas/documents/utils/contact",
	"title": "IContact",
	"description": "IContact Type",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"address": {
			"type": "string"
		},
		"phoneNumber": {
			"type": "string"
		},
		"emergencyNumber": {
			"type": "string"
		}
	},
	"additionalProperties": false,
	"required": ["name"]
